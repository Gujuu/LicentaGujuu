{
  "name": "deifratilicenta-backend",
  "version": "1.0.0",
  "description": "Backend API for Dei Frati restaurant website",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed:menu": "node scripts/seed-menu-from-static.js",
    "seed:reset": "node scripts/seed-menu-from-static.js --reset",
    "migrate:s3": "node scripts/migrate-local-uploads-to-s3.js",
    "migrate:s3:delete-local": "node scripts/migrate-local-uploads-to-s3.js --delete-local",
    "reorg:s3:dry": "node scripts/reorganize-s3-media.js",
    "reorg:s3:apply": "node scripts/reorganize-s3-media.js --apply",
    "reorg:s3:apply:delete-old": "node scripts/reorganize-s3-media.js --apply --delete-old",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "@aws-sdk/client-s3": "^3.400.0",
    "express-fileupload": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": ["restaurant", "api", "express", "mysql"]
}